select * from users;
iduser, username, password,

select * from vehicles;
idvehicle, lic_plate_number, description, creation_date, created_by, last_update_date, last_updated_by,

select * from access_types;
idaccess_type, name, description,

select * from vehicle_access;
idvehicle_access, user, vehicle, access_type,

select * from invoiceable_types;
idinvoiceable_type, name, description,

select * from invoiceables;
idinvoiceable, name, description, cost, type,

select * from maintenances;
idmaintenance, vehicle, date, notes, mileage,

select * from maintenence_invoiceables;
idmaintenence_invoiceable, maintenance, invoiceable,

